<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mapper.registryMapper">
    <insert id="apply" parameterType="registry" >
        insert into registry (name,type,untiprice,quantity,tax,money,method,person,tel,aid,flag) values (#{name},#{type},#{untiprice},#{quantity},#{tax},#{money},#{method},#{person},#{tel},#{aid},#{flag})
    </insert>
</mapper>
